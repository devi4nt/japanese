{
    "name": "japanese",
    "version": "1.0.1",
    "description": "A little project I made to help me learn japanese",
    "repository": {
        "url": "git@github.com:devi4nt/japanese.git"
    },
    "main": "index.ts",
    "stylelint": {
        "extends": "@holograph/stylelint-holo-rules",
        "rules": {
            "indentation": [
                4
            ]
        }
    },
    "prettier": "@holograph/prettier-holo-config",
    "lint-staged": {
        "*.ts": [
            "eslint"
        ]
    },
    "scripts": {
        "test": "cd cgi-bin; prove -r t",
        "prebuild": "npm install && npm update",
        "build": "npm run compile",
        "precompile": "npm run lint.ts && npm run lint.css",
        "compile": "webpack",
        "lint.ts": "eslint \"src/**/*.ts\"",
        "lint.ts.fix": "npm run lint.ts -- --fix",
        "lint": "tslint -p ./tsconfig.json",
        "lint.css": "stylelint '**/*.css'",
        "lint.css.fix": "npm run lint.css -- --fix",
        "serve": "npx http-server dist"
    },
    "devDependencies": {
        "@holograph/stylelint-holo-rules": "^3.0.1",
        "@holograph/prettier-holo-config": "^1.0.2",
        "@holograph/eslint-config-holo": "^2.0.2",
        "@types/bootstrap": "^5.0.1",
        "@types/jquery": "^3.5.5",
        "@types/popper.js": "^1.11.0",
        "clean-webpack-plugin": "^3.0.0",
        "exports-loader": "^1.1.1",
        "handlebars": "^4.7.6",
        "serialize-javascript": "^5.0.1",
        "ts-loader": "^8.0.12",
        "typescript": "^4.1.2",
        "webpack": "^5.10.0",
        "webpack-cli": "^4.2.0"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {}
}
